{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ],
  "env": {
    "ROBLOX_API_KEY": "@roblox_api_key",
    "UNIVERSE_ID": "@universe_id",
    "MESSAGING_TOPIC": "@messaging_topic"
  }
}
